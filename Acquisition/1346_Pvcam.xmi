<classes name="Pvcam"><description language="Cpp" description="This device server implements the Ccd abstract class for ccd`s that can be accessed&#10;&#9;&#9;using the PVCAM library e.g. Photometrics and Princeton cameras.&#10;&#9;&#9;&#10;&#9;&#9;This server has been tested with the following cameras :&#10;&#9;&#9;&#10;&#9;&#9;Quantix from Photometrics"><identification classFamily="Acquisition" contact="andy_gotz" platform="" bus="" manufacturer="" reference=""><keyWords>2D</keyWords></identification></description><attributes name="Exposure" attType="Scalar" description="the exposure time for the image"><dataType xsi:type="pogoDsl:DoubleType"/></attributes><attributes name="Roi" attType="Spectrum" description="coordinates of the region of interest"><dataType xsi:type="pogoDsl:IntType"/></attributes><attributes name="Binning" attType="Spectrum" description="binning factor of image"><dataType xsi:type="pogoDsl:ShortType"/></attributes><attributes name="Trigger" attType="Scalar" description="trigger mode of camera (0=internal, 1=external)"><dataType xsi:type="pogoDsl:ShortType"/></attributes><attributes name="Width" attType="Scalar" description="width of image in pixels"><dataType xsi:type="pogoDsl:IntType"/></attributes><attributes name="Height" attType="Scalar" description="height of image in pixels"><dataType xsi:type="pogoDsl:IntType"/></attributes><attributes name="Frames" attType="Scalar" description="number of images to take"><dataType xsi:type="pogoDsl:IntType"/></attributes><attributes name="Depth" attType="Scalar" description="number of bits per pixel"><dataType xsi:type="pogoDsl:ShortType"/></attributes><attributes name="FileParams" attType="Spectrum" description="the file parameters for generating file names (directory, prefix, suffix, number)"><dataType xsi:type="pogoDsl:StringType"/></attributes><attributes name="FileFormat" attType="Scalar" description="file format for saving images to disk"><dataType xsi:type="pogoDsl:StringType"/></attributes><attributes name="Image" attType="Image" description="image data"><dataType xsi:type="pogoDsl:UCharType"/></attributes><attributes name="ImageFormat" attType="Scalar" description=""><dataType xsi:type="pogoDsl:StringType"/></attributes><attributes name="ImageCounter" attType="Scalar" description=""><dataType xsi:type="pogoDsl:IntType"/></attributes><attributes name="JpegImage" attType="Spectrum" description=""><dataType xsi:type="pogoDsl:UCharType"/></attributes><attributes name="JpegQuality" attType="Scalar" description=""><dataType xsi:type="pogoDsl:ShortType"/></attributes><attributes name="JpegCompression" attType="Scalar" description=""><dataType xsi:type="pogoDsl:BooleanType"/></attributes><commands name="State" description="This command gets the device state (stored in its device_state data member) and returns it to the caller."><argin><type xsi:type="pogoDsl:VoidType"/></argin><argout><type xsi:type="pogoDsl:StateType"/></argout></commands><commands name="Status" description="This command gets the device status (stored in its device_status data member) and returns it to the caller."><argin><type xsi:type="pogoDsl:VoidType"/></argin><argout><type xsi:type="pogoDsl:StringType"/></argout></commands><commands name="Start" description="start camera exposing"><argin><type xsi:type="pogoDsl:VoidType"/></argin><argout><type xsi:type="pogoDsl:VoidType"/></argout></commands><commands name="Stop" description="stop camera exposing"><argin><type xsi:type="pogoDsl:VoidType"/></argin><argout><type xsi:type="pogoDsl:VoidType"/></argout></commands><commands name="Reset" description="reset camera"><argin><type xsi:type="pogoDsl:VoidType"/></argin><argout><type xsi:type="pogoDsl:VoidType"/></argout></commands><commands name="WriteFile" description="write image to file in the selected format, use the file parameters to generate file name"><argin><type xsi:type="pogoDsl:VoidType"/></argin><argout><type xsi:type="pogoDsl:VoidType"/></argout></commands></classes></pogoDsl:PogoSystem>