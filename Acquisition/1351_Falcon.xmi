<?xml version="1.0" encoding="ASCII"?><pogoDsl:PogoSystem xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:pogoDsl="http://www.esrf.fr/tango/pogo/PogoDsl" xmi:version="2.0">
<classes name="Falcon"><description language="Cpp" description=""><identification classFamily="Acquisition" contact="at esrf.fr - dd" platform="All Platforms" bus="rr" manufacturer="none" reference=""><keyWords>- 2D</keyWords></identification></description><attributes name="Exposure" attType="Scalar" description="the exposure time for the image"><dataType xsi:type="pogoDsl:DoubleType"/></attributes><attributes name="Trigger" attType="Scalar" description="trigger mode of camera (0=internal, 1=external)"><dataType xsi:type="pogoDsl:ShortType"/></attributes><attributes name="VideoInput" attType="Scalar" description="Selecte the video input channel"><dataType xsi:type="pogoDsl:IntType"/></attributes><attributes name="Width" attType="Scalar" description="width of image in pixels"><dataType xsi:type="pogoDsl:IntType"/></attributes><attributes name="Height" attType="Scalar" description="height of image in pixels"><dataType xsi:type="pogoDsl:IntType"/></attributes><attributes name="Depth" attType="Scalar" description="number of bits per pixel"><dataType xsi:type="pogoDsl:ShortType"/></attributes><attributes name="Frames" attType="Scalar" description="number of images to take"><dataType xsi:type="pogoDsl:IntType"/></attributes><attributes name="ImageCounter" attType="Scalar" description="Increases the count with every acquired image."><dataType xsi:type="pogoDsl:IntType"/></attributes><attributes name="ImageFormat" attType="Scalar" description="Returns the actual image format as RGB24 or Y8 for\nmonochrome images."><dataType xsi:type="pogoDsl:StringType"/></attributes><attributes name="JpegQuality" attType="Scalar" description="The quality factor of the jpeg compression used with the\nJepgImage attribute."><dataType xsi:type="pogoDsl:ShortType"/></attributes><attributes name="JpegCompression" attType="Scalar" description=""><dataType xsi:type="pogoDsl:BooleanType"/></attributes><attributes name="ColorMode" attType="Scalar" description="Switches between a color and a monochrome acquisition mode\nIf true the Image attribute returns a color image in RGB24 format.\nWhen false a monochrome image is returned in Y8 format."><dataType xsi:type="pogoDsl:BooleanType"/></attributes><attributes name="Brightness" attType="Scalar" description="Sets the brightness of the image"><dataType xsi:type="pogoDsl:IntType"/></attributes><attributes name="Contrast" attType="Scalar" description="Set the contrast of the image (luminance amplification) in the range \nof 0% to 200 %."><dataType xsi:type="pogoDsl:IntType"/></attributes><attributes name="AutoGainControl" attType="Scalar" description="Switches on or off the automatic gain control"><dataType xsi:type="pogoDsl:BooleanType"/></attributes><attributes name="ScalingFactor" attType="Scalar" description="Scaling  factor of the image."><dataType xsi:type="pogoDsl:ShortType"/></attributes><attributes name="FileFormat" attType="Scalar" description="file format for saving images to disk"><dataType xsi:type="pogoDsl:StringType"/></attributes><attributes name="FullWidth" attType="Scalar" description="width of image in pixels"><dataType xsi:type="pogoDsl:IntType"/></attributes><attributes name="FullHeight" attType="Scalar" description="height of image in pixels"><dataType xsi:type="pogoDsl:IntType"/></attributes><attributes name="MonochromeThreshold" attType="Scalar" description="An intensity threshold for monochrom images. The threshold\nwill be applied only when a monochrome image acquisition \nmode is selected. All pixel values &lt; threshold will be set to 0. \n"><dataType xsi:type="pogoDsl:IntType"/></attributes><attributes name="Roi" attType="Spectrum" description="coordinates of the region of interest"><dataType xsi:type="pogoDsl:IntType"/></attributes><attributes name="Binning" attType="Spectrum" description="Binning factor of the image in x and y"><dataType xsi:type="pogoDsl:ShortType"/></attributes><attributes name="JpegImage" attType="Spectrum" description="Read the  image in compressed jpeg format.\nThe attribute ColorMode switches between monochrome\nand color (RGB24) images."><dataType xsi:type="pogoDsl:UCharType"/></attributes><attributes name="FileParams" attType="Spectrum" description="the file parameters for generating file names (directory, prefix, suffix, number)"><dataType xsi:type="pogoDsl:StringType"/></attributes><attributes name="Image" attType="Image" description="image data"><dataType xsi:type="pogoDsl:UCharType"/></attributes><commands name="State" description="This command gets the device state (stored in its device_state data member) and returns it to the caller."><argin><type xsi:type="pogoDsl:VoidType"/></argin><argout><type xsi:type="pogoDsl:StateType"/></argout></commands><commands name="Status" description="This command gets the device status (stored in its device_status data member) and returns it to the caller."><argin><type xsi:type="pogoDsl:VoidType"/></argin><argout><type xsi:type="pogoDsl:StringType"/></argout></commands><commands name="Live" description="Start camera in live acquisition mode."><argin><type xsi:type="pogoDsl:VoidType"/></argin><argout><type xsi:type="pogoDsl:VoidType"/></argout></commands><commands name="Stop" description="stop the live acquisition mode"><argin><type xsi:type="pogoDsl:VoidType"/></argin><argout><type xsi:type="pogoDsl:VoidType"/></argout></commands><commands name="Reset" description="reset ROI, binning and scaling to the full image values"><argin><type xsi:type="pogoDsl:VoidType"/></argin><argout><type xsi:type="pogoDsl:VoidType"/></argout></commands><commands name="WriteFile" description="write image to file in the selected format, use the file parameters to generate file name"><argin><type xsi:type="pogoDsl:VoidType"/></argin><argout><type xsi:type="pogoDsl:VoidType"/></argout></commands><commands name="TGradient" description="This command makes an estimation about the  focalization of the camera.&#10;&#9;&#9;&#9;The calculation is done on the current image."><argin><type xsi:type="pogoDsl:ShortType"/></argin><argout><type xsi:type="pogoDsl:IntType"/></argout></commands><commands name="Start" description="Take a single image frame and do the necessary calculation on the acquired image.&#10;&#9;&#9;&#9;After the command is executed the image and the results can be read."><argin><type xsi:type="pogoDsl:VoidType"/></argin><argout><type xsi:type="pogoDsl:VoidType"/></argout></commands><deviceProperties name="Board_id" description="Identifies which falcon frame grabber board to use. Every board has&#10;&#9;&#9;&#9;its ID which can be set."><type xsi:type="pogoDsl:IntType"/></deviceProperties><deviceProperties name="Video_mode" description="Defines the video mode used for the camera.&#10;&#9;&#9;&#9;Can be set to PAL or NTSC."><type xsi:type="pogoDsl:StringType"/></deviceProperties><deviceProperties name="Init_Live" description="Chooses the acquisition mode at start-up."><type xsi:type="pogoDsl:BooleanType"/></deviceProperties><deviceProperties name="Init_Binning" description="Binning factor of the image applied in x and y"><type xsi:type="pogoDsl:ShortType"/></deviceProperties><deviceProperties name="Acq_sleep_time" description="The sleep time between two image acquisitions. Can be used to slow&#10;&#9;&#9;&#9;down the image acquisition and save CPU power"><type xsi:type="pogoDsl:IntType"/></deviceProperties></classes></pogoDsl:PogoSystem>