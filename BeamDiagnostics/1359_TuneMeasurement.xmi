<?xml version="1.0" encoding="ASCII"?><pogoDsl:PogoSystem xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:pogoDsl="http://www.esrf.fr/tango/pogo/PogoDsl" xmi:version="2.0">
<classes name="TuneMeasurement"><description language="Cpp" description="Tune measurement device.&#10;&#9;&#9;This device computes tune measurement by getting its data from another device attribute (currently BPM or RF),&#10;&#9;&#9;computing a Fast Fourier Transform (FFT) on it with possible averaging, and extracting&#10;&#9;&#9;the maximal value of this FFT inside a predefined interval."><identification classFamily="BeamDiagnostics" contact="buteau" platform="" bus="" manufacturer="" reference=""/></description><attributes name="NuPeakSearchStart" attType="Scalar" description="the beginning value for searching the peak value\n in the FFT that corresponds to the tune measure"><dataType xsi:type="pogoDsl:DoubleType"/></attributes><attributes name="NuPeakSearchEnd" attType="Scalar" description="the end value for searching the peak value\n in the FFT that corresponds to the tune measure"><dataType xsi:type="pogoDsl:DoubleType"/></attributes><attributes name="Nu" attType="Scalar" description=""><dataType xsi:type="pogoDsl:DoubleType"/></attributes><attributes name="NuInHigherInterval" attType="Scalar" description="sets the interval of the Nu attribute : if false Nu will be in [0, 0.5], if true Nu will be in [0.5, 1]."><dataType xsi:type="pogoDsl:BooleanType"/></attributes><attributes name="NuRaw" attType="Scalar" description="the reduced frequency of the FFT maximum searched in the interval [0, 0.5]"><dataType xsi:type="pogoDsl:DoubleType"/></attributes><attributes name="FFTNbPoints" attType="Scalar" description=""><dataType xsi:type="pogoDsl:IntType"/></attributes><attributes name="SkippedFirstSamples" attType="Scalar" description="the number of samples that are dropped in the beginning\n of the src buffer, before computing the FFT"><dataType xsi:type="pogoDsl:UShortType"/></attributes><attributes name="FFTAveraging" attType="Scalar" description="the number of successive FFT taken to compute the averaged FFT"><dataType xsi:type="pogoDsl:UShortType"/></attributes><attributes name="EstimComputTime" attType="Scalar" description="the estimated time needed to get data from the src device and compute a new FFT"><dataType xsi:type="pogoDsl:IntType"/></attributes><attributes name="ComputationPeriod" attType="Scalar" description="The internal period in millisecond at which new tune measures are produced."><dataType xsi:type="pogoDsl:IntType"/></attributes><attributes name="FFTWindowSize" attType="Scalar" description="the number of points in the src buffer that are used to compute the FFT"><dataType xsi:type="pogoDsl:IntType"/></attributes><attributes name="NuRawMagnitude" attType="Scalar" description="Amplitude of the frequency associated to NuRaw"><dataType xsi:type="pogoDsl:DoubleType"/></attributes><attributes name="FFTabs" attType="Spectrum" description="the abscissa axis of the FFTord attribute"><dataType xsi:type="pogoDsl:DoubleType"/></attributes><attributes name="FFTord" attType="Spectrum" description="the RMS power spectrum on which nu is searched"><dataType xsi:type="pogoDsl:DoubleType"/></attributes><attributes name="FFTphase" attType="Spectrum" description="Value of the phase extracted from the computd FFT"><dataType xsi:type="pogoDsl:DoubleType"/></attributes><commands name="State" description="This command gets the device state (stored in its device_state data member) and returns it to the caller."><argin><type xsi:type="pogoDsl:VoidType"/></argin><argout><type xsi:type="pogoDsl:StateType"/></argout></commands><commands name="Status" description="This command gets the device status (stored in its device_status data member) and returns it to the caller."><argin><type xsi:type="pogoDsl:VoidType"/></argin><argout><type xsi:type="pogoDsl:StringType"/></argout></commands><commands name="Start" description="Start the computation of the tune measure"><argin><type xsi:type="pogoDsl:VoidType"/></argin><argout><type xsi:type="pogoDsl:VoidType"/></argout></commands><commands name="Stop" description="Stops the computation of the tune measure"><argin><type xsi:type="pogoDsl:VoidType"/></argin><argout><type xsi:type="pogoDsl:VoidType"/></argout></commands><deviceProperties name="SrcDevice" description="Device name where the data are read"><type xsi:type="pogoDsl:StringType"/></deviceProperties><deviceProperties name="SrcDataAttrName" description="Attribute name read on SrcDevice for the data"><type xsi:type="pogoDsl:StringType"/></deviceProperties><deviceProperties name="SrcSizeAttrName" description="Attribute name to configure the number of samples in SrcDataAttrName"><type xsi:type="pogoDsl:StringType"/></deviceProperties><deviceProperties name="SrcSizeWriteEnabled" description="Tells if this device is allowed to control the SrcSizeAttrName, or if it can only read it"><type xsi:type="pogoDsl:BooleanType"/></deviceProperties><deviceProperties name="SkippedFirstSamples" description="The number of samples that are dropped in the beginning of the data src buffer, before computing the FFT"><type xsi:type="pogoDsl:ShortType"/></deviceProperties><deviceProperties name="WindowType" description="The type of window applied to the input signal. Must be one of the following : RECTANGULAR, BLACKMAN, EXACT_BLACKMAN, HAMMING, HANN, FLATTOP, BLACKMAN_HARRIS_3, BLACKMAN_HARRIS_4, BLACKMAN_HARRIS_7, LOW_SIDELOBE"><type xsi:type="pogoDsl:StringType"/></deviceProperties><deviceProperties name="FFTMinimalNbPoints" description="The minimal number of bins in the FFT.&#10;&#9;&#9;&#9;Must be a power of 2 (otherwise, rounded to the next power of 2"><type xsi:type="pogoDsl:IntType"/></deviceProperties><deviceProperties name="ComputationPeriod" description="The initial internal period in millisecond at which new tune measures are produced"><type xsi:type="pogoDsl:IntType"/></deviceProperties><deviceProperties name="AutoStart" description="if set to `true`, the computation of the tune measure starts automatically when the device is launched"><type xsi:type="pogoDsl:BooleanType"/></deviceProperties><deviceProperties name="FFTWindowSize" description="The number of points in the src buffer that are used to compute the FFT"><type xsi:type="pogoDsl:IntType"/></deviceProperties></classes></pogoDsl:PogoSystem>