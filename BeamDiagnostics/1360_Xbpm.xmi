<classes name="Xbpm"><description language="Cpp" description="This class is an universal Xbpm controller.&#10;&#9;&#9;Xpbm Currents (mA) and Beam Position X (&#239;&#191;&#189;m) and Z (&#239;&#191;&#189;m) will be processed by this class."><identification classFamily="BeamDiagnostics" contact="at nexeya.com - sebastien.gara" platform="All Platforms" bus="Not Applicable" manufacturer="none" reference=""><keyWords>BeamDiag</keyWords></identification></description><attributes name="gain" attType="Scalar" description="Gain read on the electrometer Device in &#239;&#191;&#189;A/V.\nPossible values: 0.00001 0.0001 0.001 0.01 0.1 1 10 100\n"><dataType xsi:type="pogoDsl:DoubleType"/></attributes><attributes name="enableAutoRange" attType="Scalar" description="Enables or disables electrometer gain auto range."><dataType xsi:type="pogoDsl:BooleanType"/></attributes><attributes name="current1" attType="Scalar" description="Average current on channel 0 (in measurementUnit)."><dataType xsi:type="pogoDsl:DoubleType"/></attributes><attributes name="current2" attType="Scalar" description="Average current on channel 1 (in measurementUnit)."><dataType xsi:type="pogoDsl:DoubleType"/></attributes><attributes name="current3" attType="Scalar" description="Average current on channel 2 (in measurementUnit)."><dataType xsi:type="pogoDsl:DoubleType"/></attributes><attributes name="current4" attType="Scalar" description="Average current on channel 3 (in measurementUnit)."><dataType xsi:type="pogoDsl:DoubleType"/></attributes><attributes name="intensity" attType="Scalar" description="Sum of the 4 average currents (in measurementUnit)."><dataType xsi:type="pogoDsl:DoubleType"/></attributes><attributes name="measurementUnit" attType="Scalar" description="Current measurement unit among mA, nA &#239;&#191;&#189;A. Default: &#239;&#191;&#189;A"><dataType xsi:type="pogoDsl:StringType"/></attributes><attributes name="horizontalPosition" attType="Scalar" description="Beam horizontal position in mm."><dataType xsi:type="pogoDsl:DoubleType"/></attributes><attributes name="verticalPosition" attType="Scalar" description="Beam vertical position in mm."><dataType xsi:type="pogoDsl:DoubleType"/></attributes><attributes name="computationMode" attType="Scalar" description="Beam position computation mode.\nPossible values are:\n&#239;&#191;&#189; 0: square geometry formula using corrected coefficients (Calibration and Offset tables) to compute beam position.\n&#239;&#191;&#189; 1: square geometry formula using calculated Calibration and Offset table to compute beam position.\n&#239;&#191;&#189; 2: cross geometry formula using corrected coefficients (Calibration and Offset tables) to compute beam position.\n&#239;&#191;&#189; 3: dipole geometry formula using parametred offsets and calibration factors and gain correction tables to compute beam position.\n&#239;&#191;&#189; 4: cross geometry formula using parametred offsets and calibration factors and gain correction tables to compute beam position.\n&#239;&#191;&#189; 5: square geometry formula using parametred offsets and calibration factors and gain correction tables to compute beam position.\n&#239;&#191;&#189; 6: square geometry formula using parametred offsets and calibration factors to compute beam position.\n&#239;&#191;&#189; 7: cross geometry formula using parametred offsets and calibration factors to compute beam position."><dataType xsi:type="pogoDsl:UShortType"/></attributes><commands name="State" description="This command gets the device state (stored in its device_state data member) and returns it to the caller."><argin><type xsi:type="pogoDsl:VoidType"/></argin><argout><type xsi:type="pogoDsl:StateType"/></argout></commands><commands name="Status" description="This command gets the device status (stored in its device_status data member) and returns it to the caller."><argin><type xsi:type="pogoDsl:VoidType"/></argin><argout><type xsi:type="pogoDsl:StringType"/></argout></commands><commands name="Start" description="Starts acquisition in continuous mode."><argin><type xsi:type="pogoDsl:VoidType"/></argin><argout><type xsi:type="pogoDsl:VoidType"/></argout></commands><commands name="Stop" description="Stops acquisition"><argin><type xsi:type="pogoDsl:VoidType"/></argin><argout><type xsi:type="pogoDsl:VoidType"/></argout></commands><commands name="SetUnit" description="Sets currents unit."><argin><type xsi:type="pogoDsl:UShortType"/></argin><argout><type xsi:type="pogoDsl:VoidType"/></argout></commands><deviceProperties name="ElectrometerProxyName" description="Name of the Electrometer Device proxy (ex : LoCum-4 Device)."><type xsi:type="pogoDsl:StringType"/></deviceProperties><deviceProperties name="ElectrometerGainName" description="Name of the Electrometer Device gain attribute (ex: gain)."><type xsi:type="pogoDsl:StringType"/></deviceProperties><deviceProperties name="SaiControllerProxyName" description="Name of the SaiController Device proxy."><type xsi:type="pogoDsl:StringType"/></deviceProperties><deviceProperties name="SaiControllerChan0Name" description="Name of the SaiController channel 0 attribute (ex: channel0)."><type xsi:type="pogoDsl:StringType"/></deviceProperties><deviceProperties name="SaiControllerChan1Name" description="Name of the SaiController channel 1 attribute (ex: channel1)."><type xsi:type="pogoDsl:StringType"/></deviceProperties><deviceProperties name="SaiControllerChan2Name" description="Name of the SaiController channel 2 attribute (ex: channel2)."><type xsi:type="pogoDsl:StringType"/></deviceProperties><deviceProperties name="SaiControllerChan3Name" description="Name of the SaiController channel 3 attribute (ex: channel3)."><type xsi:type="pogoDsl:StringType"/></deviceProperties><deviceProperties name="I1Offset" description="Current offset (in &#239;&#191;&#189;A) on electrode 1."><type xsi:type="pogoDsl:DoubleType"/></deviceProperties><deviceProperties name="I2Offset" description="Current offset (in &#239;&#191;&#189;A) on electrode 2."><type xsi:type="pogoDsl:DoubleType"/></deviceProperties><deviceProperties name="I3Offset" description="Current offset (in &#239;&#191;&#189;A) on electrode 3."><type xsi:type="pogoDsl:DoubleType"/></deviceProperties><deviceProperties name="I4Offset" description="Current offset (in &#239;&#191;&#189;A) on electrode 4."><type xsi:type="pogoDsl:DoubleType"/></deviceProperties><deviceProperties name="GI1" description="Gain correction (in &#239;&#191;&#189;A/V) for electrode 1."><type xsi:type="pogoDsl:DoubleType"/></deviceProperties><deviceProperties name="GI2" description="Gain correction (in &#239;&#191;&#189;A/V) for electrode 2."><type xsi:type="pogoDsl:DoubleType"/></deviceProperties><deviceProperties name="GI3" description="Gain correction (in &#239;&#191;&#189;A/V) for electrode 3."><type xsi:type="pogoDsl:DoubleType"/></deviceProperties><deviceProperties name="GI4" description="Gain correction (in &#239;&#191;&#189;A/V) for electrode 4."><type xsi:type="pogoDsl:DoubleType"/></deviceProperties><deviceProperties name="V1Offset" description="Voltage offset (in V) on electrode 1."><type xsi:type="pogoDsl:DoubleType"/></deviceProperties><deviceProperties name="V2Offset" description="Voltage offset (in V) on electrode 2."><type xsi:type="pogoDsl:DoubleType"/></deviceProperties><deviceProperties name="V3Offset" description="Voltage offset (in V) on electrode 3."><type xsi:type="pogoDsl:DoubleType"/></deviceProperties><deviceProperties name="V4Offset" description="Voltage offset (in V) on electrode 4."><type xsi:type="pogoDsl:DoubleType"/></deviceProperties><deviceProperties name="LowVoltageThreshold" description="If the input voltage of the ADC decrease this threshold (in V), the corresponding average current attribute become ALARM (i.e. if |Vmes| &lt; threshold)."><type xsi:type="pogoDsl:DoubleType"/></deviceProperties><deviceProperties name="HighVoltageThreshold" description="If the input voltage of the ADC exceed this threshold (in V), the corresponding average current attribute become ALARM (i.e. |Vmes|&gt;  threshold)."><type xsi:type="pogoDsl:DoubleType"/></deviceProperties><deviceProperties name="IntensityThreshold" description="Current out of which the Measured Intensity has a meaning (in micro-A)."><type xsi:type="pogoDsl:DoubleType"/></deviceProperties><deviceProperties name="StartAtInit" description="Launch start command at initialization time."><type xsi:type="pogoDsl:BooleanType"/></deviceProperties><deviceProperties name="EnableSigmaIntensity" description="Enables or disables standard deviation intensity computing (standardDeviationIntensity &amp; currentSpectrum attributes)."><type xsi:type="pogoDsl:BooleanType"/></deviceProperties><deviceProperties name="EnablePositionHistory" description="Enables or disables standard deviation position computing (standardDeviationPosition &amp; historyPosition attributes)"><type xsi:type="pogoDsl:BooleanType"/></deviceProperties><deviceProperties name="EnableIntensityHistory" description="Enables or disables intensity history (historyIntensity attribute)"><type xsi:type="pogoDsl:BooleanType"/></deviceProperties><deviceProperties name="EnableFluxComputing" description="Enables or disables beam flux display (flux attribute)."><type xsi:type="pogoDsl:BooleanType"/></deviceProperties><deviceProperties name="BeamEnergyProxyName" description="Name of the beam Energy Device Proxy."><type xsi:type="pogoDsl:StringType"/></deviceProperties><deviceProperties name="EnergyAttr" description="Name of the energy attribute to read from the beam energy Device Proxy."><type xsi:type="pogoDsl:StringType"/></deviceProperties><deviceProperties name="ModeX" description="List of the properties needed by the computation mode `X`.&#10;&#9;&#9;&#9;Format: {key}::{value}"><type xsi:type="pogoDsl:String[]Type"/></deviceProperties><deviceProperties name="ScanMode" description="If true, this property enables the scan mode, i.e. the polling task is disabled and data is computed on each client`s request."><type xsi:type="pogoDsl:BooleanType"/></deviceProperties><deviceProperties name="XbpmType" description="XBPM type among PSD, Quadrant. Determines the intensity calculation."><type xsi:type="pogoDsl:StringType"/></deviceProperties></classes></pogoDsl:PogoSystem>