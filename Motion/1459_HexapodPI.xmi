<?xml version="1.0" encoding="ASCII"?><pogoDsl:PogoSystem xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:pogoDsl="http://www.esrf.fr/tango/pogo/PogoDsl" xmi:version="2.0">
<classes name="HexapodPI"><description language="Cpp" description="controls hexapods from PI"><identification classFamily="Motion" contact="at synchrotron-soleil.fr - jean.coquet" platform="All Platforms" bus="Ethernet" manufacturer="PI (Physik Instrument)" reference="PI M810"><keyWords>Hexapods</keyWords></identification></description><attributes name="x" attType="Scalar" description="translation suivant X dans le repere en cours???"><dataType xsi:type="pogoDsl:DoubleType"/></attributes><attributes name="y" attType="Scalar" description="translation suivant Y dans le repere en cours???"><dataType xsi:type="pogoDsl:DoubleType"/></attributes><attributes name="z" attType="Scalar" description="translation suivant Z dans le repere en cours???"><dataType xsi:type="pogoDsl:DoubleType"/></attributes><attributes name="u" attType="Scalar" description="rotation suivant X"><dataType xsi:type="pogoDsl:DoubleType"/></attributes><attributes name="v" attType="Scalar" description="rotation suivant Y"><dataType xsi:type="pogoDsl:DoubleType"/></attributes><attributes name="w" attType="Scalar" description="rotation suivant Z"><dataType xsi:type="pogoDsl:DoubleType"/></attributes><attributes name="freeze" attType="Scalar" description="freezes the hexapod when true: \nyou can write attributes x,y,z,u,v,w without a real move.\nwhen written to false sends the command with the values written to the HXP"><dataType xsi:type="pogoDsl:BooleanType"/></attributes><attributes name="velocity" attType="Scalar" description=""><dataType xsi:type="pogoDsl:DoubleType"/></attributes><commands name="State" description="This command gets the device state (stored in its device_state data member) and returns it to the caller."><argin><type xsi:type="pogoDsl:VoidType"/></argin><argout><type xsi:type="pogoDsl:StateType"/></argout></commands><commands name="Status" description="This command gets the device status (stored in its device_status data member) and returns it to the caller."><argin><type xsi:type="pogoDsl:VoidType"/></argin><argout><type xsi:type="pogoDsl:StringType"/></argout></commands><commands name="InitializeReferencePosition" description="starts the homing process"><argin><type xsi:type="pogoDsl:VoidType"/></argin><argout><type xsi:type="pogoDsl:VoidType"/></argout></commands><commands name="GoToPosition" description="sends the hexapod to the  6 given positions values  X Y Z U V W"><argin><type xsi:type="pogoDsl:DoubleArrayType"/></argin><argout><type xsi:type="pogoDsl:VoidType"/></argout></commands><commands name="SetSystemCoordinates" description="sets the system coordinates (Pivot Point) to the 3 XYZ coordinates&#10;&#9;&#9;&#9;must be at home point (0,0,0,0,0,0) to accept the change of Pivot point"><argin><type xsi:type="pogoDsl:DoubleArrayType"/></argin><argout><type xsi:type="pogoDsl:VoidType"/></argout></commands><commands name="GetSystemCoordinates" description="gets Pivot Point coordinates"><argin><type xsi:type="pogoDsl:VoidType"/></argin><argout><type xsi:type="pogoDsl:DoubleArrayType"/></argout></commands><commands name="Stop" description="Stops all movements"><argin><type xsi:type="pogoDsl:VoidType"/></argin><argout><type xsi:type="pogoDsl:VoidType"/></argout></commands><commands name="Off" description="disble servos"><argin><type xsi:type="pogoDsl:VoidType"/></argin><argout><type xsi:type="pogoDsl:VoidType"/></argout></commands><commands name="On" description="enable servos"><argin><type xsi:type="pogoDsl:VoidType"/></argin><argout><type xsi:type="pogoDsl:VoidType"/></argout></commands><deviceProperties name="Url" description="IP Address of the HXP Controller"><type xsi:type="pogoDsl:StringType"/></deviceProperties><deviceProperties name="Port" description="port to connect to HXP"><type xsi:type="pogoDsl:IntType"/></deviceProperties><deviceProperties name="Period" description="polling period"><type xsi:type="pogoDsl:ShortType"/></deviceProperties></classes></pogoDsl:PogoSystem>