<?xml version="1.0" encoding="ASCII"?><pogoDsl:PogoSystem xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:pogoDsl="http://www.esrf.fr/tango/pogo/PogoDsl" xmi:version="2.0">
<classes name="HexapodNewport"><description language="Cpp" description="controls hexapods from Newport (HXP controllers)"><identification classFamily="Motion" contact="at synchrotron-soleil.fr - jean.coquet" platform="All Platforms" bus="Ethernet" manufacturer="Newport" reference="HXP"><keyWords>Hexapods</keyWords></identification></description><attributes name="x" attType="Scalar" description="translation suivant X dans le repere en cours???"><dataType xsi:type="pogoDsl:DoubleType"/></attributes><attributes name="y" attType="Scalar" description="translation suivant Y dans le repere en cours???"><dataType xsi:type="pogoDsl:DoubleType"/></attributes><attributes name="z" attType="Scalar" description="translation suivant Z dans le repere en cours???"><dataType xsi:type="pogoDsl:DoubleType"/></attributes><attributes name="u" attType="Scalar" description="rotation suivant X"><dataType xsi:type="pogoDsl:DoubleType"/></attributes><attributes name="v" attType="Scalar" description="rotation suivant Y"><dataType xsi:type="pogoDsl:DoubleType"/></attributes><attributes name="w" attType="Scalar" description="rotation suivant Z"><dataType xsi:type="pogoDsl:DoubleType"/></attributes><attributes name="freeze" attType="Scalar" description="freezes the hexapod when true: \nyou can write attributes x,y,z,u,v,w without a real move.\nwhen written to false sends the command with the values written to the HXP"><dataType xsi:type="pogoDsl:BooleanType"/></attributes><commands name="State" description="This command gets the device state (stored in its device_state data member) and returns it to the caller."><argin><type xsi:type="pogoDsl:VoidType"/></argin><argout><type xsi:type="pogoDsl:StateType"/></argout></commands><commands name="Status" description="This command gets the device status (stored in its device_status data member) and returns it to the caller."><argin><type xsi:type="pogoDsl:VoidType"/></argin><argout><type xsi:type="pogoDsl:StringType"/></argout></commands><commands name="InitializeReferencePosition" description="starts the homing process"><argin><type xsi:type="pogoDsl:VoidType"/></argin><argout><type xsi:type="pogoDsl:VoidType"/></argout></commands><commands name="GoToPosition" description="sends the hexapod to the  6 given positions values  X Y Z U V W"><argin><type xsi:type="pogoDsl:DoubleArrayType"/></argin><argout><type xsi:type="pogoDsl:VoidType"/></argout></commands><commands name="SetSystemCoordinates" description="sets the system coordinates (Work, Base, Tool) and the 6 coordinates&#10;&#9;&#9;&#9;form :&#10;&#9;&#9;&#9;Work, 0,-1.0, -0.003,27,33,134.5"><argin><type xsi:type="pogoDsl:StringType"/></argin><argout><type xsi:type="pogoDsl:VoidType"/></argout></commands><commands name="GetSystemCoordinates" description="gets the system coordinates (Work, Base, Tool) and the 6 coordinates&#10;&#9;&#9;&#9;input form : Work&#10;&#9;&#9;&#9;output form :  Work,0,-1.0, -0.003,27,33,134.5"><argin><type xsi:type="pogoDsl:StringType"/></argin><argout><type xsi:type="pogoDsl:StringType"/></argout></commands><commands name="Stop" description="Stops all movements"><argin><type xsi:type="pogoDsl:VoidType"/></argin><argout><type xsi:type="pogoDsl:VoidType"/></argout></commands><commands name="Kill" description="kill all movements&#10;&#9;&#9;&#9;deinitializes the Hexapod (which means an InitializeReferencePosition is mandatory after this operation)"><argin><type xsi:type="pogoDsl:VoidType"/></argin><argout><type xsi:type="pogoDsl:VoidType"/></argout></commands><commands name="DefinePosition" description="defines the position as the XYZUVW"><argin><type xsi:type="pogoDsl:DoubleArrayType"/></argin><argout><type xsi:type="pogoDsl:VoidType"/></argout></commands><deviceProperties name="Url" description="IP Address of the HXP Controller"><type xsi:type="pogoDsl:StringType"/></deviceProperties><deviceProperties name="Port" description="port to connect to HXP"><type xsi:type="pogoDsl:ShortType"/></deviceProperties><deviceProperties name="Period" description="polling period"><type xsi:type="pogoDsl:ShortType"/></deviceProperties><deviceProperties name="PositioningType" description="type of positioning [Absolute|Relative]&#10;&#9;&#9;&#9;if Absolute, positioning performs GroupMoveAbsolute (Absolute positions)&#10;&#9;&#9;&#9;if Relative, positioning performs GroupMoveRelative (Requested Positions - Current Positions)&#10;&#9;&#9;&#9;Default : no default"><type xsi:type="pogoDsl:StringType"/></deviceProperties></classes></pogoDsl:PogoSystem>