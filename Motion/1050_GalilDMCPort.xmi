    </description>
    <deviceProperties name="ControlDevicePath" description="Path to the GalilDMCControl device providing the controller connection.">
      <type xsi:type="pogoDsl:StringType"/>
      <status abstract="false" inherited="false" concrete="true" concreteHere="true"/>
    </deviceProperties>
    <deviceProperties name="PortType" description="May be AI, AO, DI or DO for analog or digital input or output ports. Note that analog outputs are only available via Modbus and have to be set up manually.">
      <type xsi:type="pogoDsl:StringType"/>
      <status abstract="false" inherited="false" concrete="true" concreteHere="true"/>
    </deviceProperties>
    <deviceProperties name="PortAddress" description="Address to the port. Possible values are:&#xA;DI 1-16 general input, 17-48 shared extended I/O, 81-96 aux encoder repurposed as inputs, 1000-8999 Modbus slave bits;&#xA;DO 1-16 general output, 17-48 shared extended I/O, 1000-8999 Modbus slave bits;&#xA;AI 1-8 analogue inputs corresponding axis A-H;&#xA;AO 1000-8999 Modbus slave ports.&#xA;Modbus slaves have to be set up manually.">
      <type xsi:type="pogoDsl:ShortType"/>
      <status abstract="false" inherited="false" concrete="true" concreteHere="true"/>
    </deviceProperties>
    <deviceProperties name="Range" description="Sets the input range for analog inputs. May be 1 for +-5V, 2 for +-10V, 3 for 0-5V or 4 for 0-10V.">
      <type xsi:type="pogoDsl:UShortType"/>
      <status abstract="false" inherited="false" concrete="true" concreteHere="true"/>
      <DefaultPropValue>2</DefaultPropValue>
    </deviceProperties>
    <deviceProperties name="Differential" description="Setting this for analog inputs 1, 3, 5 or 7, configures those inputs as the differential input relative to input 2, 4, 6 and 8 respectively.">
      <type xsi:type="pogoDsl:BooleanType"/>
      <status abstract="false" inherited="false" concrete="true" concreteHere="true"/>
      <DefaultPropValue>false</DefaultPropValue>
