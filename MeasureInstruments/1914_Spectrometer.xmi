<?xml version="1.0" encoding="ASCII"?><pogoDsl:PogoSystem xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:pogoDsl="http://www.esrf.fr/tango/pogo/PogoDsl" xmi:version="2.0">
<classes name="Spectrometer"><description language="Cpp" description="This class controls an Ocean Optics Spectrometer interfaced in USB"><identification classFamily="MeasureInstruments" contact="at nexeya.com - sebastien.gara" platform="Windows" bus="USB" manufacturer="Ocean Optics" reference="USB2000+"><keyWords>Acquisition</keyWords></identification></description><attributes name="averagingNb" attType="Scalar" description="Sets the number of acquisitions to be used for spectrum calculation"><dataType xsi:type="pogoDsl:UShortType"/></attributes><attributes name="boxcar" attType="Scalar" description="sets the number of pixels to be averaged together"><dataType xsi:type="pogoDsl:UShortType"/></attributes><attributes name="integrationTime" attType="Scalar" description="sets the integration time in milliseconds"><dataType xsi:type="pogoDsl:IntType"/></attributes><attributes name="lampEnable" attType="Scalar" description="sets the lamp status:\n0 light off, 1 light on"><dataType xsi:type="pogoDsl:BooleanType"/></attributes><attributes name="acquisitionCount" attType="Scalar" description=""><dataType xsi:type="pogoDsl:UIntType"/></attributes><attributes name="triggerMode" attType="Scalar" description="Sets the triggering mode:\n0= normal (free running) mode = INTERNAL_SINGLE\n1= software trigger = INTERNAL_TRIGGERED_SINGLE\n2= external hardware level trigger mode = EXTERNAL_MULTI\n3= external synchronization trigger mode = EXTERNAL_GATE\n4= external harware edge trigger mode = EXTERNAL_SINGLE\n"><dataType xsi:type="pogoDsl:UShortType"/></attributes><attributes name="maxBufferDepth" attType="Scalar" description="max buffer depth when running a continuous acquisition."><dataType xsi:type="pogoDsl:UIntType"/></attributes><attributes name="nbGates" attType="Scalar" description="Expected number of gates (in Mode 2). 0 means infinite, until STOP command."><dataType xsi:type="pogoDsl:UIntType"/></attributes><attributes name="acqDelay" attType="Scalar" description="time delay (ms) before starting a new acquisition (in START mode)"><dataType xsi:type="pogoDsl:UIntType"/></attributes><attributes name="lambda" attType="Spectrum" description="wavelengths values"><dataType xsi:type="pogoDsl:DoubleType"/></attributes><attributes name="intensity" attType="Spectrum" description="spectral intensity (in counts) resulting from one acquisition"><dataType xsi:type="pogoDsl:DoubleType"/></attributes><commands name="State" description="This command gets the device state (stored in its device_state data member) and returns it to the caller."><argin><type xsi:type="pogoDsl:VoidType"/></argin><argout><type xsi:type="pogoDsl:StateType"/></argout></commands><commands name="Status" description="This command gets the device status (stored in its device_status data member) and returns it to the caller."><argin><type xsi:type="pogoDsl:VoidType"/></argin><argout><type xsi:type="pogoDsl:StringType"/></argout></commands><commands name="Start" description="Starts multiple spectrum acquisitions according to chosen mode. Returns spectrum until a &quot;Stop&quot; command is sent by user."><argin><type xsi:type="pogoDsl:VoidType"/></argin><argout><type xsi:type="pogoDsl:VoidType"/></argout></commands><commands name="Stop" description="stops the current acquisition"><argin><type xsi:type="pogoDsl:VoidType"/></argin><argout><type xsi:type="pogoDsl:VoidType"/></argout></commands><commands name="GetFirmwareVersion" description="returns version number of code running on the microcontroller"><argin><type xsi:type="pogoDsl:VoidType"/></argin><argout><type xsi:type="pogoDsl:StringType"/></argout></commands><commands name="Snap" description="Start an acquisition for framesNb spectra.&#10;&#9;&#9;&#9;The final spectrum is the averaging of all spectra"><argin><type xsi:type="pogoDsl:VoidType"/></argin><argout><type xsi:type="pogoDsl:VoidType"/></argout></commands><commands name="GetIntensity" description="Get intensity spectrum by index"><argin><type xsi:type="pogoDsl:UIntType"/></argin><argout><type xsi:type="pogoDsl:DoubleArrayType"/></argout></commands><deviceProperties name="TriggerMode" description="Sets the triggering mode:&#10;&#9;&#9;&#9;0= normal (free running) mode = INTERNAL_SINGLE&#10;&#9;&#9;&#9;1= software trigger = INTERNAL_TRIGGERED_SINGLE&#10;&#9;&#9;&#9;2= external hardware level trigger mode = EXTERNAL_MULTI&#10;&#9;&#9;&#9;3= external synchronization trigger mode = EXTERNAL_GATE&#10;&#9;&#9;&#9;4= external harware edge trigger mode = EXTERNAL_SINGLE"><type xsi:type="pogoDsl:ShortType"/></deviceProperties><deviceProperties name="SpectrometerIndex" description="USB index if there is several spectrometers"><type xsi:type="pogoDsl:ShortType"/></deviceProperties><deviceProperties name="StopAcqOnSaturation" description="This property enables/disables the acquisition stop on spectrum saturation."><type xsi:type="pogoDsl:BooleanType"/></deviceProperties><deviceProperties name="AlarmOnSaturation" description="Device change to ALARM state if saturated"><type xsi:type="pogoDsl:BooleanType"/></deviceProperties></classes></pogoDsl:PogoSystem>