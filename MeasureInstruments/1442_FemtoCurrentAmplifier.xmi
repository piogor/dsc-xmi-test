utes><attributes name="gainSelectedNum" attType="Scalar" description=""><dataType xsi:type="pogoDsl:DoubleType"/></attributes><commands name="State" description="This command gets the device state (stored in its device_state data member) and returns it to the caller."><argin><type xsi:type="pogoDsl:VoidType"/></argin><argout><type xsi:type="pogoDsl:StateType"/></argout></commands><commands name="Status" description="This command gets the device status (stored in its device_status data member) and returns it to the caller."><argin><type xsi:type="pogoDsl:VoidType"/></argin><argout><type xsi:type="pogoDsl:StringType"/></argout></commands><commands name="IncreaseGain" description="Increase the Gain (max = 6)."><argin><type xsi:type="pogoDsl:VoidType"/></argin><argout><type xsi:type="pogoDsl:VoidType"/></argout></commands><commands name="DecreaseGain" description="Decrease the Gain (min = 0)."><argin><type xsi:type="pogoDsl:VoidType"/></argin><argout><type xsi:type="pogoDsl:VoidType"/></argout></commands><commands name="AutoSearchGain" description="Automatically search Gain:&#10;&#9;&#9;&#9;Set 1st Gain&#10;&#9;&#9;&#9;Check if Overload&#10;&#9;&#9;&#9;...."><argin><type xsi:type="pogoDsl:VoidType"/></argin><argout><type xsi:type="pogoDsl:VoidType"/></argout></commands><deviceProperties name="DIOBoardName" description="Name of the DIO board device server"><type xsi:type="pogoDsl:StringType"/></deviceProperties><deviceProperties name="CouplingLine" description="Line of the Coupling (AC/DC)."><type xsi:type="pogoDsl:StringType"/></deviceProperties><deviceProperties name="OverloadLine" description="Line of the Overload"><type xsi:type="pogoDsl:StringType"/></deviceProperties><deviceProperties name="GainModeLine" description="Line of the Gain Mode (HS/LN)."><type xsi:type="pogoDsl:StringType"/></deviceProperties><deviceProperties name="OutputSignalLine" description="Line of the Output Signal"><type xsi:type="pogoDsl:StringType"/></deviceProperties><deviceProperties name="LSBGainLine" description="Line of the LSB Gain"><type xsi:type="pogoDsl:StringType"/></deviceProperties><deviceProperties name="MidSBGainLine" description="Line of the Mid SB Gain"><type xsi:type="pogoDsl:StringType"/></deviceProperties><deviceProperties name="MSBGainLine" description="Line of the MSB Gain"><type xsi:type="pogoDsl:StringType"/></deviceProperties><deviceProperties name="FemtoType" description="Type of the Femto Current Amplifier: (Default = 2)&#10;&#9;&#9;&#9;1 -&gt; DHPCA-100&#10;&#9;&#9;&#9;2 -&gt; DLPCA-200"><type xsi:type="pogoDsl:ShortType"/></deviceProperties><deviceProperties name="UBWL10MHzLine" description="Line of the Upper BandWidth Limit 10 MHz."><type xsi:type="pogoDsl:StringType"/></deviceProperties><deviceProperties name="UBWL1MHzLine" description="Line of the Upper BandWidth Limit 1 MHz."><type xsi:type="pogoDsl:StringType"/></deviceProperties></classes></pogoDsl:PogoSystem>